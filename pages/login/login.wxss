/*login*/

.login-banner {
  height: 40rpx;
  margin-top: 46rpx;
  display: flex;
  justify-content: center;
  flex: 0 0 auto
}

.login-banner-item {
  margin: 0 46rpx;
  line-height: 40rpx;
  font-size: 30rpx;
  position: relative;
}

.login-banner-item-active {
  color: #FD6363;
  /* position: relative; */
}

.login-banner-item-active::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 0;
  background: #FD6363;
  width: 100%;
  height: 1px;
}

.login-container {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
  background-color: #EEEFF5;
}

.login-form {
  display: flex;
  flex-direction: column;
  align-items: center;
  flex: 1 0 auto;
}

.login-btn {
  margin-top: 30rpx;
  color: #fff;
  background: #FD6363;
  border-radius: 10rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
}

.login-btn-focu {
  bottom: none;
}

/* 弹窗 */

.toast-login {
  position: absolute;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background: rgba(0, 0, 0, .3);
  z-index: 999;
}

.toast-login-invite {
  width: 600rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  background-color: #FF5D68;
  background-size: contain;
  background-repeat: no-repeat;
  background-image: url('data:image/png;base64,');
}

.toast-login-invite-title {
  color: #970B2E;
  margin-top: 50rpx;
  font-size: 58rpx;
}

.toast-login-invite-title-text {
  color: #fff;
  font-size: 30rpx;
  font-weight: lighter;
  margin: 10rpx 0 30rpx;
}

.toast-login-invite-text {
  color: #FD6363;
  background: #fff;
  font-size: 30rpx;
  height: 90rpx;
  width: 600rpx;
  line-height: 90rpx;
  margin-top: 54rpx;
  text-align: center;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
}

.require {
  position: relative;
}

.require::before {
  content: '*';
  position: absolute;
  left: 5rpx;
  top: 0;
  bottom: 0;
  line-height: 90rpx;
  color: red;
}

/* 重写登录注册样式 */

.input-contain {
  margin: 30rpx 0;
}

.input-item {
  height: 100rpx;
  background-color: #fff;
  display: flex;
  position: relative;
  padding: 0 30rpx;
}

.input-item:not(:last-of-type)::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 30rpx;
  right: 30rpx;
  background: #ccc;
  /* width: 100%; */
  height: 2rpx;
}

input {
  color: #ccc;
  height: 100rpx;
  line-height: 100rpx;
  flex: 1;
  font-size: 30rpx;
}

.input-item-text {
  font-size: 36rpx;
  line-height: 100rpx;
  width: 144rpx;
  margin-right: 100rpx;
}

