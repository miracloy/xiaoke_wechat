/*memberInfo*/
.zan-panel{border-top:none}
.zan-cell::after{left:0;background:#f2f2f2}
.zan-cell--access .zan-cell__ft::after{border-color:#fb6264;height:18rpx;width:18rpx;border-width:2rpx 2rpx 0 0}
.memberInfo{margin-top:0}
.memberInfo .zan-cell{overflow:hidden;height:170rpx;padding:10rpx 24rpx}
.memberInfo .zan-cell__bd{overflow:hidden}
.memberInfo .zan-cell__bd .p{width:100rpx;height:100rpx}
.memberInfo .photo{width:100rpx;height:100rpx;border-radius:100rpx;-webkit-border-radius:100rpx;-moz-border-radius:100rpx;overflow:hidden;margin-right:20rpx;background-color:#f0f0f0}
.memberInfo .zan-cell__bd .i text{display:block;max-width:480rpx;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}
.memberInfo .i .name{font-size:32rpx}
.memberInfo .i .tel,.memberInfo .i .money{color:#666;font-size:28rpx}

/*memberList*/
.memberList{border-bottom:1px solid #f2f2f2}
.memberList .zan-cell .icon{width:40rpx;height:40rpx;margin:0 25rpx;float:left}
.memberList .zan-cell{padding:25rpx 24rpx;height:40rpx;line-height:40rpx;font-size:32rpx;border-top:none}
.memberList .zan-cell--access .zan-cell__ft::after{border-color:#ccc;width:16rpx;height:16rpx}

/*cartOpeat*/
.cartOpeat{margin:30rpx 25rpx;}
.cartOpeat .zan-btn{background-color:#fd6363;border-color:#fd6363;margin:0}